/dts-v1/;
#include <dt-bindings/i2c/i2c.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/rkfb/rk_fb.h>
#include "rk3188.dtsi"
/ {
	model = "Radxa Rock Pro";
	compatible = "radxa,rock", "rockchip,rk3188", "rockchip,rk3066", "rockchip,rk3066a";

	aliases {
		lcdc0 = &lcdc0;
		lcdc1 = &lcdc1;
	};

	memory@60000000 {
		device_type = "memory";
		reg = <0x60000000 0x80000000>;
	};

	gpio-keys {
		compatible = "gpio-keys";
		autorepeat;

		power {
			//gpios = <&gpio0 RK_PA4 GPIO_ACTIVE_LOW>;
			gpios = <&gpio0 RK_PA4 &pcfg_pull_down>;
			linux,code = <KEY_POWER>;
			label = "GPIO Key Power";
			linux,input-type = <1>;
			wakeup-source;
			debounce-interval = <100>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";

		green: led-0 {
			label = "rock:green:user1";
			//gpios = <&gpio0 RK_PB4 GPIO_ACTIVE_LOW>;
			gpios = <&gpio0 RK_PB4 &pcfg_pull_down>;
			default-state = "off";
		};

		blue: led-1 {
			label = "rock:blue:user2";
			//gpios = <&gpio0 RK_PB6 GPIO_ACTIVE_LOW>;
			gpios = <&gpio0 RK_PB6 &pcfg_pull_down>;
			default-state = "off";
		};

		sleep: led-2 {
			label = "rock:red:power";
			//gpios = <&gpio0 RK_PB7 GPIO_ACTIVE_HIGH>;
			gpios = <&gpio0 RK_PB7 &pcfg_pull_up>;
			default-state = "off";
		};
	};

	sound {
		compatible = "simple-audio-card";
		simple-audio-card,name = "SPDIF";

		simple-audio-card,dai-link@1 {
			cpu { sound-dai = <&spdif>; };
			codec { sound-dai = <&spdif_out>; };
		};
	};

	spdif_out: spdif-out {
		compatible = "linux,spdif-dit";
		#sound-dai-cells = <0>;
	};

	ir_recv: gpio-ir-receiver {
		compatible = "gpio-ir-receiver";
		//gpios = <&gpio0 RK_PB2 GPIO_ACTIVE_LOW>;
		gpios = <&gpio0 RK_PB2 &pcfg_pull_down>;
		pinctrl-names = "default";
		pinctrl-0 = <&ir_recv_pin>;
	};

	vcc_otg: usb-otg-regulator {
		compatible = "regulator-fixed";
		enable-active-high;
		//gpio = <&gpio2 RK_PD7 GPIO_ACTIVE_HIGH>;
		gpio = <&gpio2 RK_PD7 &pcfg_pull_up>;
		pinctrl-names = "default";
		pinctrl-0 = <&otg_vbus_drv>;
		regulator-name = "otg-vbus";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-always-on;
		regulator-boot-on;
	};

	vcc_sd0: sdmmc-regulator {
		compatible = "regulator-fixed";
		regulator-name = "sdmmc-supply";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		//gpio = <&gpio3 RK_PA1 GPIO_ACTIVE_LOW>;
		gpio = <&gpio3 RK_PA1 &pcfg_pull_down>;
		pinctrl-names = "default";
		pinctrl-0 = <&sdmmc_pwr>;
		startup-delay-us = <100000>;
		vin-supply = <&vcc_io>;
	};

	vcc_host: usb-host-regulator {
		compatible = "regulator-fixed";
		enable-active-high;
		//gpio = <&gpio0 RK_PA3 GPIO_ACTIVE_HIGH>;
		gpio = <&gpio0 RK_PA3 &pcfg_pull_up>;
		pinctrl-names = "default";
		pinctrl-0 = <&host_vbus_drv>;
		regulator-name = "host-pwr";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-always-on;
		regulator-boot-on;
	};

	vsys: vsys-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vsys";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-boot-on;
	};

	codec_hdmi_i2s: codec-hdmi-i2s {
		compatible = "hdmi-i2s";
	};

	codec_hdmi_spdif: codec-hdmi-spdif {
		compatible = "hdmi-spdif";
	};

	rockchip-hdmi-i2s {
		compatible = "rockchip-hdmi-i2s";
		dais {
			dai0 {
				audio-codec = <&codec_hdmi_i2s>;
				audio-controller = <&i2s0>;
				format = "i2s";
				//continuous-clock;
				//bitclock-inversion;
				//frame-inversion;
				//bitclock-master;
				//frame-master;
			};
		};
	};

	rockchip-hdmi-spdif {
		compatible = "rockchip-hdmi-spdif";
	};

	rockchip-rk1000 {
		compatible = "rockchip-rk1000";
		dais {
			dai0 {
				audio-codec = <&rk1000_codec>;
				audio-controller = <&i2s0>;
				format = "i2s";
			};
		};
	};

	fb: fb{
		compatible = "rockchip,rk-fb";
		rockchip,disp-mode = <ONE_DUAL>;
		//rockchip,disp-mode = <DUAL>;
		//rockchip,disp-mode = <NO_DUAL>;
		//rockchip,uboot-logo-on = <1>;
		//rockchip,disp-policy = <DISPLAY_POLICY_BOX>;
	};
	rk_screen: rk_screen{
		compatible = "rockchip,screen";
		//display-timings = <&disp_timings>;
		#include <dt-bindings/display/screen-timing/lcd-box.dtsi>
	};

	lcdc0: lcdc0@1010c000 {
		compatible = "rockchip,rk3188-lcdc";
		rockchip,prop = <PRMRY>;
		reg = <0x1010c000 0x2000>;
		interrupts = <GIC_SPI 9 IRQ_TYPE_LEVEL_HIGH>;
		//clocks = <&clk_gates6 0>, <&dclk_lcdc0>, <&clk_gates6 1>, <&sclk_lcdc0>, <&pd_vop>, <&clk_cpll>;
		//clock-names = "aclk_lcdc", "dclk_lcdc", "hclk_lcdc", "sclk_lcdc", "pd_lcdc", "sclk_pll";

		clocks = <&cru ACLK_LCDC0>, <&cru DCLK_LCDC0>, <&cru HCLK_LCDC0>;
		clock-names = "aclk_lcdc0", "dclk_lcdc0", "hclk_lcdc0";

		rockchip,iommu-enabled = <1>;
		status = "okay";

		/*pinctrl-names = "default", "gpio";
		pinctrl-0 = <&lcdc0_lcdc>;
		pinctrl-1 = <&lcdc0_gpio>;*/

		rockchip,fb-win-map = <FB_DEFAULT_ORDER>;
		//rockchip,fb-win-map = <FB0_WIN0_FB1_WIN1_FB2_WIN2>;
		power_ctr0: power_ctr {
			rockchip,debug = <1>;
			lcd_en0: lcd_en {
				rockchip,power_type = <GPIO>;
				//gpios = <&gpio3 GPIO_C4 GPIO_ACTIVE_HIGH>;
				//gpios = <&gpio0 GPIO_B0 &pcfg_pull_none>;
				//gpios = <&gpio0 RK_PB0 &pcfg_pull_none>;
				gpios = <&gpio0 RK_PB0 &pcfg_pull_down>;
				//gpios = <0 RK_PB0 RK_FUNC_GPIO &pcfg_pull_none>;
				rockchip,delay = <10>;
			};
			lcd_cs0: lcd_cs {
				rockchip,power_type = <GPIO>;
				//gpios = <&gpio2 GPIO_D6 GPIO_ACTIVE_HIGH>;
				//gpios = <&gpio2 RK_PD6 GPIO_ACTIVE_HIGH>;
				gpios = <&gpio2 RK_PD6 &pcfg_pull_up>;
				rockchip,delay = <10>;
			};
			/*lcd_rst:lcd_rst {
				rockchip,power_type = <REGULATOR>;
				rockchip,delay = <5>;
			};*/
		};
	};

	lcdc1: lcdc1@1010e000 {
		compatible = "rockchip,rk3188-lcdc";
		rockchip,prop = <PRMRY>;
		reg = <0x1010e000 0x2000>;
		interrupts = <GIC_SPI 9 IRQ_TYPE_LEVEL_HIGH>;
		//clocks = <&clk_gates6 0>, <&dclk_lcdc1>, <&clk_gates6 1>, <&sclk_lcdc1>, <&pd_vop>, <&clk_cpll>;
		//clock-names = "aclk_lcdc", "dclk_lcdc", "hclk_lcdc", "sclk_lcdc", "pd_lcdc", "sclk_pll";

		clocks = <&cru ACLK_LCDC1>, <&cru DCLK_LCDC1>, <&cru HCLK_LCDC1>;
		clock-names = "aclk_lcdc1", "dclk_lcdc1", "hclk_lcdc1";

		rockchip,iommu-enabled = <1>;
		//status = "disabled";
		status = "okay";

		/*pinctrl-names = "default", "gpio";
		pinctrl-0 = <&lcdc1_lcdc>;
		pinctrl-1 = <&lcdc1_gpio>;*/

		rockchip,fb-win-map = <FB_DEFAULT_ORDER>;
		//rockchip,fb-win-map = <FB0_WIN0_FB1_WIN1_FB2_WIN2>;
		power_ctr1: power_ctr {
			rockchip,debug = <1>;
			rockchip,mirror = <NO_MIRROR>;
			lcd_en1: lcd_en {
				rockchip,power_type = <GPIO>;
				//gpios = <&gpio3 GPIO_C4 GPIO_ACTIVE_HIGH>;
				//gpios = <&gpio0 GPIO_B0 &pcfg_pull_none>;
				//gpios = <&gpio0 RK_PB0 &pcfg_pull_none>;
				gpios = <&gpio0 RK_PB0 &pcfg_pull_down>;
				//gpios = <0 RK_PB0 RK_FUNC_GPIO &pcfg_pull_none>;
				rockchip,delay = <10>;
			};
			lcd_cs1:lcd_cs {
				rockchip,power_type = <GPIO>;
				//gpios = <&gpio2 GPIO_D6 GPIO_ACTIVE_HIGH>;
				gpios = <&gpio2 RK_PD6 &pcfg_pull_up>;
				rockchip,delay = <10>;
			};
			/*lcd_rst:lcd_rst {
				rockchip,power_type = <REGULATOR>;
				rockchip,delay = <5>;
			};*/
		};
	};

	//3128
};

&cpu0 {
	cpu-supply = <&vdd_arm>;
};

&cpu1 {
	cpu-supply = <&vdd_arm>;
};

&cpu2 {
	cpu-supply = <&vdd_arm>;
};

&cpu3 {
	cpu-supply = <&vdd_arm>;
};

&mmc0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&sd0_clk>, <&sd0_cmd>, <&sd0_cd>, <&sd0_bus4>;
	vmmc-supply = <&vcc_sd0>;

	bus-width = <4>;
	cap-mmc-highspeed;
	cap-sd-highspeed;
	disable-wp;
};

&pwm1 {
	status = "okay";
};

&pwm2 {
	status = "okay";
};

&pwm3 {
	status = "okay";
};

&spdif {
	status = "okay";
};

&uart0 {
	status = "okay";
};

&uart1 {
	status = "okay";
};

&uart2 {
	status = "okay";
};

&uart3 {
	status = "okay";
};

&usbphy {
	status = "okay";
};

&usb_host {
	status = "okay";
};

&usb_otg {
	status = "okay";
};

&wdt {
	status = "okay";
};

/*&i2s {
	status = "okay";
};*/

&i2s0 {
	status = "okay";
};

&emac {
	status = "okay";

	pinctrl-names = "default";
	pinctrl-0 = <&emac_xfer>, <&emac_mdio>, <&phy_int>;

	phy = <&phy0>;
	phy-supply = <&vcc_rmii>;

	phy0: ethernet-phy@0 {
		reg = <0>;
		interrupt-parent = <&gpio3>;
		interrupts = <RK_PD2 IRQ_TYPE_LEVEL_LOW>;
	};
};

&i2c1 {
	status = "okay";
	clock-frequency = <400000>;

	rtc@51 {
		compatible = "haoyu,hym8563";
		reg = <0x51>;
		interrupt-parent = <&gpio0>;
		interrupts = <RK_PB5 IRQ_TYPE_EDGE_FALLING>;
		pinctrl-names = "default";
		pinctrl-0 = <&rtc_int>;
		#clock-cells = <0>;
		clock-output-names = "xin32k";
	};

	act8846: act8846@5a {
		compatible = "active-semi,act8846";
		reg = <0x5a>;
		status = "okay";
		system-power-controller;

		pinctrl-names = "default";
		pinctrl-0 = <&act8846_dvs0_ctl>;

		vp1-supply = <&vsys>;
		vp2-supply = <&vsys>;
		vp3-supply = <&vsys>;
		vp4-supply = <&vsys>;
		inl1-supply = <&vcc_io>;
		inl2-supply = <&vsys>;
		inl3-supply = <&vsys>;

		regulators {
			vcc_ddr: REG1 {
				regulator-name = "VCC_DDR";
				regulator-min-microvolt = <1200000>;
				regulator-max-microvolt = <1200000>;
				regulator-always-on;
			};

			vdd_log: REG2 {
				regulator-name = "VDD_LOG";
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <1000000>;
				regulator-always-on;
			};

			vdd_arm: REG3 {
				regulator-name = "VDD_ARM";
				regulator-min-microvolt = <875000>;
				regulator-max-microvolt = <1350000>;
				regulator-always-on;
			};

			vcc_io: REG4 {
				regulator-name = "VCC_IO";
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vdd_10: REG5 {
				regulator-name = "VDD_10";
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <1000000>;
				regulator-always-on;
			};

			vdd_hdmi: REG6 {
				regulator-name = "VDD_HDMI";
				regulator-min-microvolt = <2500000>;
				regulator-max-microvolt = <2500000>;
				regulator-always-on;
			};

			vcc18: REG7 {
				regulator-name = "VCC_18";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-always-on;
			};

			vcca_33: REG8 {
				regulator-name = "VCCA_33";
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vcc_rmii: REG9 {
				regulator-name = "VCC_RMII";
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
			};

			vccio_wl: REG10 {
				regulator-name = "VCCIO_WL";
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vcc_18: REG11 {
				regulator-name = "VCC18_IO";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-always-on;
			};

			vcc28: REG12 {
				regulator-name = "VCC_28";
				regulator-min-microvolt = <2800000>;
				regulator-max-microvolt = <2800000>;
				regulator-always-on;
			};
		};
	};
};

&i2c4 {
	status = "okay";
	rk1000_control@40 {
		compatible = "rockchip,rk1000_control", "rockchip,rk1000-ctl";
		/*reg = <0x40>;
		//gpio-reset = <&gpio7 GPIO_C5 GPIO_ACTIVE_LOW>;
		//gpio-reset = <&gpio7 GPIO_C5 &pcfg_pull_down>;
		gpio-reset = <&gpio3 RK_PB7 &pcfg_pull_down>;
		#clocks = <&clk_i2s>, <&clk_i2s_out>;
		#clock-names = "i2s_clk","i2s_mclk";
		#pinctrl-names = "default";
		#pinctrl-0 = <&i2s_mclk>;
		status = "okay";*/
		status = "disabled";
	};
	rk1000_tve@42 {
		compatible = "rockchip,rk1000_tve", "rockchip,rk1000-tve";
		/*reg = <0x42>;
		#rockchip,source = <0>; //0: LCDC0; 1: LCDC1
		#rockchip,prop = <PRMRY>;//<EXTEND>
		rockchip,source = <0>; //0: LCDC0; 1: LCDC1
		rockchip,prop = <EXTEND>;//<>
		status = "okay";*/
		status = "disabled";
	};
	rk1000_codec: rk1000_codec@60 {
		compatible = "rockchip,rk1000_codec";
		/*reg = <0x60>;
		//spk_ctl_io = <&gpio7 GPIO_A5 GPIO_ACTIVE_LOW>;
		spk_ctl_io = <&gpio7 GPIO_A5 &pcfg_pull_down>;
		boot_depop = <1>;
		pa_enable_time = <5000>;
		status = "okay";*/
		status = "disabled";
	};
};

&pinctrl {
	pcfg_output_low: pcfg-output-low {
		output-low;
	};

	act8846 {
		act8846_dvs0_ctl: act8846-dvs0-ctl {
			rockchip,pins = <3 RK_PD3 RK_FUNC_GPIO &pcfg_output_low>;
		};
	};

	hym8563 {
		rtc_int: rtc-int {
			rockchip,pins = <0 RK_PA0 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	lan8720a  {
		phy_int: phy-int {
			rockchip,pins = <3 RK_PD2 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	ir-receiver {
		ir_recv_pin: ir-recv-pin {
			rockchip,pins = <0 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	sd0 {
		sdmmc_pwr: sdmmc-pwr {
			rockchip,pins = <3 RK_PA1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	usb {
		host_vbus_drv: host-vbus-drv {
			rockchip,pins = <0 RK_PA3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		otg_vbus_drv: otg-vbus-drv {
			rockchip,pins = <2 RK_PD7 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	display {
		lcd_cs: lcd-cs {
			rockchip,pins = <3 RK_PD6 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		lcd_en: lcd-en {
			rockchip,pins = <0 RK_PB0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};


&gpu {
	assigned-clocks = <&cru ACLK_GPU>;
	assigned-clock-rates = <300000000>;
	mali-supply = <&vdd_log>;
	status = "okay";
};
